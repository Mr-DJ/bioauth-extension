{
    "manifest_version": 2,
    "name": "Biopass",
    "version": "1.0",
    "description": "The browser extension for the Biopass password manager.",
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "test.js"
            ],
            "permissions": [
                "activeTab",
                "contextMenus",
                "clipboardWrite",
                "nativeMessaging",
                "notifications",
                "storage",
                "tabs",
                "webNavigation",
                "webRequest",
                "webRequestBlocking",
                "https://*/*",
                "http://*/*",
                "https://api.github.com/"
            ]
        }
    ]
}